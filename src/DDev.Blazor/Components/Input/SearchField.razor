@inherits FieldBase<string>

<FieldTemplate Label="@Label" ValueId="@Id" Disabled="@Disabled" Empty="string.IsNullOrEmpty(Value)" Invalid="IsFieldInvalid">
    <input class="text-field__value" type="text" autocomplete="off" disabled="@Disabled" id="@Id" @bind="Value" @bind:event="oninput" />
</FieldTemplate>